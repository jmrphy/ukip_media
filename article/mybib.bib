@article{walgrave_making_2004,
	Abstract = {In Flanders, the Dutch-speaking part of Belgium, one of the strongest extreme right parties of Europe thrives: the Vlaams Blok (the Flemish Bloc). The basic question of this article is straightforward: Do the Flemish media contribute to the success of the Vlaams Blok by emphasizing the themes of the party? The theoretical argument is twofold: agenda setting by the media and issue ownership by parties. The issues the Vlaams Blok owns are determined using two sources: its electoral manifestoes and its electorate's motivations to vote for the party. This leads to four issues: Flemish nationalism, immigrant topics, antipolitics issues, and crime-related themes. Using a vast media data set covering three newspapers and two TV stations and stretching over 10 years (1991-2000), we examine to what extent these issues were covered. The analysis shows that especially immigrant topics and crime receive extensive and growing media attention, and time series analysis shows that this rise parallels the electoral growth of the Vlaams Blok. The media could be considered co-responsible for the Vlaams Blok's upsurge.},
	Author = {Walgrave, Stefaan and Swert, Knut De},
	File = {Full Text PDF:C\:\\Users\\Daniel\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ioe9aotx.default\\zotero\\storage\\6TNKUE3D\\WALGRAVE and SWERT - 2004 - The Making of the (Issues of the) Vlaams Blok.pdf:application/pdf;Snapshot:C\:\\Users\\Daniel\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ioe9aotx.default\\zotero\\storage\\KHVJZIXP\\10584600490522743.html:text/html},
